{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "name": "fidelity",
    "slug": "fidelity",
    "splash": {
      "image": "./assets/Logo.png",
      "backgroundColor": "#3D195B",
      "resizeMode": "contain"
    },
    "extra": {
      "supportsRTL": true,
      "router": {
        "origin": false
      }
    }
  }
}