{
  "expo": {
    "scheme": "com.hamzamushtaq840.fidelity",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "name": "fidelity",
    "slug": "fidelity",
    "splash": {
      "image": "./assets/Logo.png",
      "backgroundColor": "#3D195B",
      "resizeMode": "contain"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.hamzamushtaq840.fidelity",
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ece4802f-2810-4a13-a420-0f60138b4ea3"
      }
    }
  }
}
